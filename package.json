{
  "name": "amida-auth-microservice",
  "version": "1.0.0",
  "description": "Microservice for a lightweight auth application using JWT",
  "author": "Jacob Sachs <jacob@amida.com>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=6.9.1",
    "npm": ">=2.15.11",
    "yarn": ">=0.20.3"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "create_db": "sequelize db:create",
    "delete_db": "sequelize db:drop",
    "clean": "rimraf coverage dist",
    "jenkins": "NODE_ENV=test yarn delete_db && NODE_ENV=test yarn create_db && NODE_ENV=test yarn migrate && NODE_ENV=test yarn test",
    "lint": "esw -c .eslintrc *.js server config . --color --fix",
    "lint:watch": "yarn lint -- --watch",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "precommit": "yarn lint && yarn test",
    "prepublish": "npm run snyk-protect",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "seed": "sequelize db:seed:all",
    "serve": "node dist/index.js",
    "snyk-protect": "snyk protect",
    "start": "nodemon --exec babel-node -- src/index.js",
    "start:debug": "cross-env DEBUG=amida-auth-microservice:* yarn start",
    "test": "NODE_ENV=test mocha --ui bdd --reporter spec --colors --compilers js:@babel/register tests --recursive -t 10000",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "test:coverage": "cross-env NODE_ENV=test istanbul cover _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register src/tests --recursive",
    "test:watch": "yarn test -- --watch"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "body-parser": "1.18.2",
    "chai": "^3.4.1",
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "del": "^2.2.0",
    "dotenv": "^4.0.0",
    "eslintrc": "^1.0.6",
    "express": "4.16.0",
    "express-actuator": "^1.1.0",
    "express-jwt": "5.3.0",
    "express-jwt-permissions": "1.2.1",
    "express-validation": "1.0.1",
    "express-winston": "2.5.0",
    "gulp": "3.9.1",
    "gulp-load-plugins": "^1.2.0",
    "helmet": "3.8.2",
    "http-status": "^0.2.0",
    "joi": "13.0.0",
    "jsonwebtoken": "8.0.0",
    "lodash": "4.17.5",
    "method-override": "^2.3.5",
    "moment": "2.19.3",
    "morgan": "1.9.0",
    "nodemailer": "4.1.0",
    "owasp-password-strength-test": "1.3.0",
    "passport": "0.4.0",
    "passport-facebook": "2.1.1",
    "passport-jwt": "3.0.0",
    "pg": "6.1.6",
    "pg-hstore": "2.3.2",
    "rand-token": "^0.4.0",
    "run-sequence": "^1.1.5",
    "sequelize": "^4.40.0",
    "snyk": "^1.73.0",
    "supertest": "^3.0.0",
    "swagger-stats": "^0.95.6",
    "uuid": "3.1.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "chai-datetime": "1.5.0",
    "codacy-coverage": "2.0.2",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.3",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "gulp-babel": "6.1.2",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-util": "^3.0.7",
    "husky": "^0.13.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "3.2.0",
    "nodemon": "^1.18.0",
    "rimraf": "^2.6.0",
    "sequelize-cli": "^5.4.0"
  },
  "snyk": true
}
