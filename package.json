{
  "name": "amida-auth-microservice",
  "version": "2.7.0",
  "description": "Microservice for a lightweight auth application using JWT",
  "author": "Jacob Sachs <jacob@amida.com>",
  "main": "dist/index.js",
  "private": false,
  "engines": {
    "node": ">=8.14.0",
    "npm": ">=6.4.0",
    "yarn": ">=1.12.0"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "create_db": "sequelize db:create",
    "delete_db": "sequelize db:drop",
    "clean": "rimraf coverage dist",
    "jenkins": "cross-env NODE_ENV=test yarn delete_db; cross-env NODE_ENV=test yarn create_db && cross-env NODE_ENV=test yarn migrate && cross-env NODE_ENV=test yarn test",
    "lint": "esw src test --color",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "prepublish": "snyk protect",
    "report-coverage": "coveralls < ./coverage/lcov.info",
    "seed": "sequelize db:seed:all",
    "serve": "node dist/index.js",
    "start": "nodemon --exec babel-node -- src/index.js",
    "start:debug": "cross-env DEBUG=amida-auth-microservice:* yarn start",
    "test": "cross-env NODE_ENV=test mocha --opts .mocharc",
    "test:check-coverage": "yarn test:coverage && nyc check-coverage",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --opts .mocharc",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "body-parser": "1.18.2",
    "compression": "1.7.1",
    "cookie-parser": "1.4.3",
    "cors": "2.8.1",
    "dotenv": "^4.0.0",
    "express": "4.16.0",
    "express-actuator": "^1.1.0",
    "express-jwt-permissions": "1.2.1",
    "express-validation": "1.0.1",
    "express-winston": "2.5.0",
    "helmet": "3.8.2",
    "http-status": "^0.2.0",
    "joi": "13.0.0",
    "jsonwebtoken": "8.0.0",
    "lodash": "4.17.11",
    "method-override": "3.0.0",
    "moment": "2.19.3",
    "nodemailer": "4.1.0",
    "owasp-password-strength-test": "1.3.0",
    "passport": "0.4.0",
    "passport-facebook": "2.1.1",
    "passport-jwt": "4.0.0",
    "pg": "6.1.6",
    "pg-hstore": "2.3.2",
    "rand-token": "^0.4.0",
    "sequelize": "^4.40.0",
    "swagger-stats": "^0.95.6",
    "uuid": "3.1.0",
    "winston": "^3.0.0",
    "winston-json-formatter": "^0.9.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "babel-plugin-source-map-support": "^2.0.0",
    "chai": "^3.4.1",
    "chai-datetime": "1.5.0",
    "coveralls": "3.0.2",
    "cross-env": "3.1.3",
    "eslint": "3.16.1",
    "eslint-config-airbnb-base": "7.1.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-watch": "2.1.14",
    "husky": "1.2.1",
    "mocha": "3.2.0",
    "nodemon": "^1.18.0",
    "nyc": "^13.0.0",
    "rimraf": "^2.6.0",
    "sequelize-cli": "^5.4.0",
    "snyk": "^1.73.0",
    "source-map-support": "0.5.9",
    "supertest": "^3.0.0"
  },
  "snyk": true
}
